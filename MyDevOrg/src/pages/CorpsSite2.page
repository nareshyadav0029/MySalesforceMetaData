<apex:page controller="CorpsSiteController" showHeader="false" sidebar="false" standardStylesheets="false">
    <apex:form >
        <style>
           .container{
               width:1024px;
               margin-left:100px;
           }
            .Free{
                float:left;
                width:180px;
                height:35px;
                margin-left:20px;
                background-color:#4F575A;
            }
            .Logo{
                float:left;
                margin-top:0px;
            }
            .header{
                display:inline-block;
                float:left;
                background-color:#4F575A;
                width:650px;
            }
            .header ul{
                float:left;
                height:8px;
                margin-top:12px;
            }
            .header ul li{
                display:inline-block;
                float:left;
                padding-left:20px;
                font-size:12px;
                font-family: Arial, Helvetica, sans-serif;
            }
            .header ul li a{
                text-decoration:none;
                color:#ffffff;
            }
            .header ul li a:hover{
                color:#FB9D14;
            }
            .LeftMenu{
                 clear:both;
                 float:left;
                 margin-left:200px;
            }
            .LeftMenu ul {
                background-color:#FB9D14;
                width:160px; 
                padding:0px;
                margin-top:10px; 
            }
            .LeftMenu ul li{
                display:Block;
                border-bottom:5px solid #ffffff;
                padding:5px;
            }
            .LeftMenu ul li a{
                text-decoration:none;
                color:#ffffff;
                text-align:center;
                display:Block;
            }
            .LeftMenu ul li a:hover{
                background-color:#ea8b2a;
                color:#ffffff;
            }
            .Form{
                float:left;
                margin-top:10px;
                margin-left:100px;
                background:#ea8b2a;
                border-radius:5px;
             }
             .Form h2{
                 color:#ffffff;
                 padding:10px;
                 font-family: Arial, Helvetica, sans-serif;
             }
        </style>
        <title>Coaching Coarps</title>
        <div class="container">
            <div class="Free">
                
            </div>
            
            <div class="Logo">
                <a href="#"><img src="{!$Resource.CoachingCorpsLogo}" style="padding-bottom:15px"/></a>
            </div>
            
            <div class="header">
                    <ul>
                        <li><a href="#"><b>ABOUT US</b></a></li>
                        <li><a href="#"><b>COACH</b></a></li>
                        <li><a href="#"><b>PARTNER</b></a></li>
                        <li><a href="#"><b>ADVOCATE</b></a></li>
                        <li><a href="#"><b>BLOG</b></a></li>
                        <li><a href="#"><b>RESOURCES</b></a></li>
                        <li><a href="#"><b>SUPPORT US</b></a></li>
                   </ul>
            </div>
            
            <div class="Form">
                <h2 align="center">Thank You For Registration !!!</h2>
            </div>
            
            <div class="LeftMenu">
               <ul> 
                  <li><a href="#">BECOME A COACH</a></li>
                  <li><a href="#">GET A COACH</a></li>
                  <li><a href="#">START A CHAPTER</a></li>
                  <li><a href="#">DONATE</a></li> 
              </ul>
            </div>
            
        </div>
